/**app.wxss**/
page{
  font-size: 28rpx;
  color:#333;
  background-color: #f5f7f9;
  width:100%;
}
.fy-container{
  padding:30rpx 20rpx;
  background-color: #fff;
}
.left{
  float: left;
}
.clearfix{
  content: '';
  display: table;
  clear: both;
  zoom: 1;
}
.flex{
  display: flex;
}
.x-center{
  justify-content: center; 
}
.x-between{
  justify-content: space-between;
}
.x-start{
  justify-content: flex-start;
}
.y-center{
  align-items: center;
}
.y-top{
  align-items: flex-start;
}

/* 尾部 */
.footer-module{
  line-height: 60rpx;
  text-align: center;
  font-size: 20rpx;
}

/* 字体引入 */

@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1534410227785'); /* IE9*/
  src: url('iconfont.eot?t=1534410227785#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('iconfont.ttf?t=1534410227785') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('iconfont.svg?t=1534410227785#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

