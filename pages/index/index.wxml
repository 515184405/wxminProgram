<!-- 轮播模块 -->
<swiper indicator-color='rgba(255,255,255,1)' indicator-active-color="{{theme}}" indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" circular="{{circular}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}" wx:key="">
    <swiper-item>
      <image mode='widthFix' src="{{item}}" class="slide-image"/>
    </swiper-item>
  </block>
</swiper>
<!-- 导航模块 -->
<view class='nav-box fy-container'>
  <view wx:for="{{8}}" wx:key="" class='nav-item'>
    <image class='nav-item-img' mode='aspectFill' src='http://uploads.5068.com/allimg/1803/153-1P319144048.jpg'></image>
    <text class='nav-item-text'>儿童照片</text>
  </view>
</view>
<!-- 模块内容 -->
