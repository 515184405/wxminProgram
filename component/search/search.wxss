.fy-relative{
  position:relative;
}
.fy-search{
  height:88rpx;
  border-radius: 88rpx;
  font-size: 28rpx;
  padding:0 80rpx 0 40rpx;
  background-color: #f3f5f7;
  color:#8f939c;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color:#8f939c; opacity:1; 
}

::-moz-placeholder { /* Mozilla Firefox 19+ */
    color:#8f939c;opacity:1;
}

input:-ms-input-placeholder{
    color:#8f939c;opacity:1;
}

input::-webkit-input-placeholder{
    color:#8f939c;opacity:1;
}
.fy-search-icon{
  position:absolute;
  right:40rpx;
  height:30rpx;
  width:30rpx;
  bottom:31rpx;
}